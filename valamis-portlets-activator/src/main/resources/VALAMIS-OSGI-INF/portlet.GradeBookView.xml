<?xml version="1.0"?>

<component name="com.arcusys.valamis.web.portlet.GradebookView" immediate="true">

    <implementation class="com.arcusys.valamis.web.portlet.GradebookView"/>

    <service>
        <provide interface="javax.portlet.Portlet"/>
    </service>


    <property name="com.liferay.portlet.display-category" type="String" value="Valamis Analytics"/>
    <property name="javax.portlet.portlet-name" type="String" value="com_arcusys_valamis_web_portlet_GradebookView"/>
    <property name="javax.portlet.info.title" type="String" value="Gradebook Viewer"/>
    <property name="javax.portlet.display-name" type="String" value="Gradebook Viewer"/>
    <property name="javax.portlet.resource-bundle" type="String" value="content.Language"/>

    <property name="javax.portlet.portlet-mode" type="String" value="text/html;view"/>
    <property name="javax.portlet.security-role-ref" type="String" value="guest,power-user,user"/>
    <property name="javax.portlet.expiration-cache" type="Integer" value="0"/>


    <property name="com.liferay.portlet.requires-namespaced-parameters" type="String" value="false"/>
    <property name="com.liferay.portlet.ajaxable" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.icon" type="String" value="/img/logo.png"/>

    <property name="com.liferay.portlet.show-portlet-access-denied" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.preferences-unique-per-layout" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.preferences-owned-by-group" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.private-request-attributes" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.private-session-attributes" type="Boolean" value="false"/>
    <property name="com.liferay.portlet.add-default-resource" type="Boolean" value="true"/>

    <property name="com.liferay.portlet.header-portlet-javascript" type="String">

        /js/gradebook-all.js

    </property>

    <property name="com.liferay.portlet.footer-portlet-css" type="String" value="/css/valamis.css"/>
    <property name="com.liferay.portlet.css-class-wrapper" type="String" value="valamis"/>

</component>